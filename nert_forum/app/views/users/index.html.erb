<div>
  <h2>NERT Forum</h2>
  <% @users.each do |user|%>
    <div>
      <h3><%= link_to user.name, user_path(user) %></h3>
      <%#= image_tag user.photo %>
      Neighborhood: <%= user.neighborhood %><br>
    </div>
    <div>
      <p><%= link_to 'Delete User', user_path(user), method: :delete, :data => {:confirm => 'Are you sure you want to delete this User?'}, class: 'waves-effect waves-light btn' %></p>
    </div>
    <hr>
  <% end %>
</div>